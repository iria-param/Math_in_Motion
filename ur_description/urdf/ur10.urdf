<?xml version="1.0"?>
<robot name="ur10">
  
  <!-- UR10 CB3 Robot Description for PyBullet -->
  <!-- Based on Universal Robots UR10 CB3 DH Parameters -->
  <!-- Reference: UR10 Technical Specifications -->
  
  <link name="base_link">
    <inertial>
      <mass value="4.0"/>
      <origin xyz="0 0 0.05"/>
      <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://ur_description/meshes/ur10/visual/base.dae" scale="1 1 1"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://ur_description/meshes/ur10/collision/base.stl" scale="1 1 1"/>
      </geometry>
    </collision>
  </link>

  <!-- Joint 1: Shoulder Pan -->
  <joint name="shoulder_pan_joint" type="revolute">
    <parent link="base_link"/>
    <child link="shoulder_link"/>
    <origin xyz="0 0 0.089159" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-6.283" upper="6.283" effort="330.0" velocity="2.16"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>

  <link name="shoulder_link">
    <inertial>
      <mass value="3.7"/>
      <origin xyz="0 0.02561 0"/>
      <inertia ixx="0.010" ixy="0.0" ixz="0.0" iyy="0.010" iyz="0.0" izz="0.010"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://ur_description/meshes/ur10/visual/shoulder.dae" scale="1 1 1"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://ur_description/meshes/ur10/collision/shoulder.stl" scale="1 1 1"/>
      </geometry>
    </collision>
  </link>

  <!-- Joint 2: Shoulder Lift -->
  <joint name="shoulder_lift_joint" type="revolute">
    <parent link="shoulder_link"/>
    <child link="upper_arm_link"/>
    <origin xyz="0 0.13585 0" rpy="1.5707963 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-6.283" upper="6.283" effort="330.0" velocity="2.16"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>

  <link name="upper_arm_link">
    <inertial>
      <mass value="8.393"/>
      <origin xyz="0 -0.02561 0.425"/>
      <inertia ixx="0.22" ixy="0.0" ixz="0.0" iyy="0.22" iyz="0.0" izz="0.020"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://ur_description/meshes/ur10/visual/upperarm.dae" scale="1 1 1"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://ur_description/meshes/ur10/collision/upperarm.stl" scale="1 1 1"/>
      </geometry>
    </collision>
  </link>

  <!-- Joint 3: Elbow -->
  <joint name="elbow_joint" type="revolute">
    <parent link="upper_arm_link"/>
    <child link="forearm_link"/>
    <origin xyz="0 -0.1197 0.425" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-6.283" upper="6.283" effort="150.0" velocity="3.15"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>

  <link name="forearm_link">
    <inertial>
      <mass value="2.275"/>
      <origin xyz="0 0 0.196"/>
      <inertia ixx="0.049" ixy="0.0" ixz="0.0" iyy="0.049" iyz="0.0" izz="0.004"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://ur_description/meshes/ur10/visual/forearm.dae" scale="1 1 1"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://ur_description/meshes/ur10/collision/forearm.stl" scale="1 1 1"/>
      </geometry>
    </collision>
  </link>

  <!-- Joint 4: Wrist 1 -->
  <joint name="wrist_1_joint" type="revolute">
    <parent link="forearm_link"/>
    <child link="wrist_1_link"/>
    <origin xyz="0 0 0.39225" rpy="1.5707963 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-6.283" upper="6.283" effort="54.0" velocity="3.15"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>

  <link name="wrist_1_link">
    <inertial>
      <mass value="1.219"/>
      <origin xyz="0 0.127 0"/>
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://ur_description/meshes/ur10/visual/wrist1.dae" scale="1 1 1"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://ur_description/meshes/ur10/collision/wrist1.stl" scale="1 1 1"/>
      </geometry>
    </collision>
  </link>

  <!-- Joint 5: Wrist 2 -->
  <joint name="wrist_2_joint" type="revolute">
    <parent link="wrist_1_link"/>
    <child link="wrist_2_link"/>
    <origin xyz="0 0.127 0" rpy="-1.5707963 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-6.283" upper="6.283" effort="54.0" velocity="3.15"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>

  <link name="wrist_2_link">
    <inertial>
      <mass value="1.219"/>
      <origin xyz="0 0 0.09475"/>
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://ur_description/meshes/ur10/visual/wrist2.dae" scale="1 1 1"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://ur_description/meshes/ur10/collision/wrist2.stl" scale="1 1 1"/>
      </geometry>
    </collision>
  </link>

  <!-- Joint 6: Wrist 3 -->
  <joint name="wrist_3_joint" type="revolute">
    <parent link="wrist_2_link"/>
    <child link="wrist_3_link"/>
    <origin xyz="0 0 0.09475" rpy="1.5707963 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-6.283" upper="6.283" effort="54.0" velocity="3.15"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>

  <link name="wrist_3_link">
    <inertial>
      <mass value="0.1879"/>
      <origin xyz="0 0.0743 0"/>
      <inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0001"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://ur_description/meshes/ur10/visual/wrist3.dae" scale="1 1 1"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://ur_description/meshes/ur10/collision/wrist3.stl" scale="1 1 1"/>
      </geometry>
    </collision>
  </link>

  <!-- Tool Flange (End Effector) -->
  <joint name="ee_fixed_joint" type="fixed">
    <parent link="wrist_3_link"/>
    <child link="ee_link"/>
    <origin xyz="0 0.0823 0" rpy="0 0 0"/>
  </joint>

  <link name="ee_link"/>

</robot>
